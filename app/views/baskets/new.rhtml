<%= render(:partial => "topics/content_wrapper_start" , :locals => { :classes =>
"full-width notabs" }) %>

<% @title = 'New basket' -%>

<h2><%= h(@title) -%></h2>

<% form_for :basket, :url => {:action => 'create'} do |form| %>
  <%= render :partial => 'form', :object => form %>
  <%= submit_tag "Create", :class => "save-button" %>
<% end %>

<%= render(:partial => "topics/content_wrapper_end" ) %>

