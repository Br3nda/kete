<% # This is where the actual forme the new topic is displayed, dynamically created in partial %>
<% @title = 'New topic' -%>

<h2><%= h(@title) -%></h2>

<% form_tag :action => 'create' do %>
  <%= render :partial => 'form' %>
  <div style="margin:0;margin:0"><%= submit_tag "Create", :class => "save-button" %></div>
<% end %>

<%= link_to_cancel %>

<%= render(:partial => "content_wrapper_end" ) %>
